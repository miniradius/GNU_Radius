AT_SETUP([tunnel attributes (proxy)])
AT_KEYWORDS([proxy-tunnel])

RA_CHECK([send auth Access-Request User-Name = "tunnel" \
                         User-Password = "test" 
          expect Access-Accept Service-Type = Framed-User \
		       Framed-Protocol = PPTP \
		       Tunnel-Password = tUnnEl])


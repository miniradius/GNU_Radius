else

   for path in /usr/local/lib/mysql /usr/lib/mysql
   do
      LIBS="$save_LIBS -lm -L$path -lmysqlclient"
      cat >conftest.$ac_ext <<_ACEOF
/* confdefs.h.  */
_ACEOF
cat confdefs.h >>conftest.$ac_ext
cat >>conftest.$ac_ext <<_ACEOF
/* end confdefs.h.  */

